package TestFiles;
	



import javafx.application.Application;
import javafx.stage.Stage;


public class Main extends Application {

	public static final byte[] OSCILLOSCOPE = {(byte) 0b11000000};
	public static final byte[] MULTIMETER = {(byte) 0b10000000};
	public static final byte[] FUNCTION = {(byte) 0b01000000 };
	public static final byte[] STOP = {(byte) 0b00000000, (byte) 0b11111111, (byte) 0b00000000};
	
	@Override
	public void start(Stage primaryStage) {
		try {
			// start a small interface to choose the pic and create a connection
			testConnection con = TestUI.start();
			con.send(OSCILLOSCOPE);
			con.send(MULTIMETER);
			con.send(FUNCTION);
			con.send(STOP);
			
		} catch(Exception e) {
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args) {
		launch(args);
	}
}
